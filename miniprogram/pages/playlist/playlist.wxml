<swiper indicator-dots="true" autoplay="true" class="swiper-container">
  <block wx:for="{{swiperImgUrls}}" wx:key="url">
    <swiper-item>
      <image src="{{item.url}}" mode="" class="img"></image>
    </swiper-item>
  </block>
</swiper>

<view class="playlist-container">
  <block wx:for="{{playlist}}" wx:key="_id">
    <r-playlist playlist="{{item}}"></r-playlist>
  </block>
</view>
